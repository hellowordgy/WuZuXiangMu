@model XiangMu.MVC.Model.Withs

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>WithsCreate</title>
   
</head>
<body>
    @using (Html.BeginForm("WithsCreates", "Withs", FormMethod.Post, new { @enctype = "multipart/form-data" })) 
